{
  "name": "devapt-core-common",
  "version": "2.1.2",
  "description": "Developpers Application Toolkit - common features - build application (backend, frontend, RESTfull, API, distributed nodes, reactive...) from configuration",
  "author": {
    "name": "Luc BORIES",
    "email": "luc.bories@gmail.com",
    "url": "https://github.com/lucbories"
  },
  "contributors": [],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucbories/devapt-core-common.git"
  },
  "bugs": {
    "url": "git+https://github.com/lucbories/devapt-core-common/issues"
  },
  "homepage": "https://github.com/lucbories/devapt-core-common#readme.md",
  "main": "dist/index.js",
  "engines": {
    "node": ">=4.4.4"
  },
  "directories": {
    "dist": "./dist"
  },
  "scripts": {
    "build": "gulp",
    "assets": "",
    "resources": "",
    "bundle": "unix2dos package.json",
    "test": "",
    "test:todo": "mocha --recursive --reporter spec --compilers js:babel-register src/test",
    "test:empty": "mocha --reporter spec --compilers js:babel-register",
    "test:watch": "npm run test -- --watch",
    "release-develop": "npm run build && npm run test && git commit -am $npm_package_version && git push common develop",
    "release-master": "npm run merge && git tag $npm_package_version && git push --tags common master && npm publish && git checkout develop",
    "patch": "npm version patch && unix2dos package.json",
    "minor": "npm version minor && unix2dos package.json",
    "major": "npm version major && unix2dos package.json",
    "merge": "git checkout master && git merge develop && git push common master",
    "save-develop": "git commit -am \"save current changes\" && git push common develop",
    "save-master": "npm run merge && git checkout develop"
  },
  "browser": [],
  "keywords": [
    "server",
    "browser",
    "backend",
    "frontend",
    "api",
    "rest",
    "devapt",
    "toolkit",
    "stateful",
    "responsive",
    "reactive",
    "declarative",
    "distributed",
    "redux"
  ],
  "dependencies": {
    "async": "^2.6.1",
    "babel-core": "^6.26.3",
    "baconjs": "^0.7.95",
    "cache-manager": ">=2.1.2",
    "es6-symbol": ">=3.1.0",
    "html-entities": "^1.2.1",
    "html-to-vdom": ">=0.7.0",
    "immutable": "^3.8.2",
    "lodash": "^4.17.11",
    "lowdb": "^1.0.0",
    "mustache": "^3.0.0",
    "node-cache": "^4.2.0",
    "node-forge": "^0.7.6",
    "path-is-absolute": ">=1.0.0",
    "ramda": "^0.24.1",
    "randomatic": "~>3.0.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "redux-undo": ">=0.5.0",
    "semver": "^5.6.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "vdom-as-json": ">=1.0.9",
    "vdom-parser": ">=1.3.4",
    "vdom-virtualize": ">=2.0.0",
    "virtual-dom": ">=2.1.1",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babelify": "^9.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-immutable": ">=1.3.0",
    "debounce": "^1.2.0",
    "del": ">=3.0.0",
    "fs.extra": ">=1.3.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.1",
    "gulp-cached": ">=1.1.0",
    "gulp-changed": "^3.2.0",
    "gulp-concat": ">=2.6.0",
    "gulp-livereload": "^4.0.0",
    "gulp-load-plugins": ">=1.2.0",
    "gulp-remember": "^1.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.7",
    "mocha": ">=5.2.0",
    "parallelshell": "^2.0.0",
    "requirejs": "^2.3.6",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
